<UserControl x:Class="eHPS.ServiceDeployer.Pages.APIConfigItem"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:mui="http://firstfloorsoftware.com/ModernUI"
             mc:Ignorable="d" Height="680" Width="700"
             d:DesignHeight="680" d:DesignWidth="700">
    <Grid Style="{StaticResource ContentRoot}">
        <ScrollViewer>
            <TextBox x:Name="TextBox" Height="23" TextWrapping="Wrap" Text="TextBox" Width="120"/>
        </ScrollViewer>
    </Grid>
</UserControl>
